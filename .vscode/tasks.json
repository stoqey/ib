{
	"version": "2.0.0",
	"tasks": [
		{
      "label": "Build and Watch Project",
      "type": "shell",
      "command": "npm",
      "args": [
        "--silent",
        "run",
        "build:watch"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$tsc-watch"
    },
	]
}
